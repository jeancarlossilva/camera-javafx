    <children>
      <ImageView fx:id="imgWebCamCapturedImage" fitHeight="247.0" fitWidth="527.0" layoutX="8.0" layoutY="46.0" pickOnBounds="true" preserveRatio="true" />
      <FlowPane fx:id="fpBottomPane" layoutX="34.0" layoutY="301.0" prefHeight="113.0" prefWidth="444.0">
